#!/bin/bash
INFORMIXDIR=/usr/informix
export INFORMIXDIR
PATH=$PATH:$INFORMIXDIR/bin
export PATH
INFORMIXSERVER=ol_arche
export INFORMIXSERVER
ONCONFIG=onconfig.ol_arche
export ONCONFIG
LD_LIBRARY_PATH=$INFORMIXDIR/lib:$INFORMIXDIR/lib/esql:$INFORMIXDIR/lib/tools:/lib
export LD_LIBRARY_PATH
TERMCAP=$INFORMIXDIR/etc/termcap
export TERMCAP
IFMX_UNDOC_B168163=1
export IFMX_UNDOC_B168163
DBDELIMITER="|"
export DBDELIMITER
DBDATE=dmy4/
export DBDATE
