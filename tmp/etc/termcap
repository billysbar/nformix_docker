	:ZB:ZA=%?%p3%t%{1}%;%?%p4%t%{2}%+%;%Sz%?%Gz%Ga%=%!%t\E[0%?%p3%t;5%;%?%p4%t;4%;m%;%Gz%Sa%?%p1%{0}%=%t%{7}%e%p1%{1}%=%t%{3}%e%p1%{2}%=%t%{5}%e%p1%{3}%=%t%{1}%e%p1%{4}%=%t%{6}%e%p1%{5}%=%t%{2}%e%p1%{6}%=%t%{4}%e%p1%{7}%=%t%{0}%;%'0'%+%Sc%p1%{7}%?%p2%t%Sx%Sy%Gc%'0'%e%Sy%Sx%'0'%Gc%;%?%Gx%Gf%=%!%t\Ea%c%e%Sd%;%?%Gy%Gb%=%!%t\Eb%c%e%Sd%;%Gx%Sf%Gy%Sb:

# ZA=
#	%?%p3%t%{1}%;		# blink?
#	%?%p4%t%{2}%+%;		# underline?
#	%Sz
#	%?%Gz%Ga%=%!%t
#	    \E[0		# output lead in
#	    %?%p3%t;5%;		# output blink
#	    %?%p4%t;4%;		# output underline
#	    m			# end of attribute set
#	%;
#	%Gz%Sa			# save attributes
#	%?%p1%{0}%=%t%{7}	# encode color number
#	%e%p1%{1}%=%t%{3}
#	%e%p1%{2}%=%t%{5}
#	%e%p1%{3}%=%t%{1}
#	%e%p1%{4}%=%t%{6}
#	%e%p1%{5}%=%t%{2}
#	%e%p1%{6}%=%t%{4}
#	%e%p1%{7}%=%t%{0}
#	%;
#	%'0'%+%Sc
#	%p1%{7}
#	%?%p2%t%Sx%Sy%Gc%'0'	# foreground in x / background in y
#	     %e%Sy%Sx%'0'%Gc	# push background then foreground
#	%;
#	%?%Gx%Gf%=%!%t\Ea%c%e%Sd%;	# output foreground color
#	%?%Gy%Gb%=%!%t\Eb%c%e%Sd%;	# output background color
#	%Gx%Sf%Gy%Sb			# save colors
#
#
# ID Systems Corporation
#
# id231 
# Note that the auto-wrap on the id231 dosen't qualify as "am".
# It dosen't move the cursor till the 81st character is output
#
