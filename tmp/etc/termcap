# ZG#0:				# no video attribute character
# ZA=\E[0			# sgr escape sequence 
#	%?%p1%{0}%=%t;37	# push ";37" for white forground
#	%e%p1%{1}%=%t;33	# push ";33" for yellow forground
#	%e%p1%{2}%=%t;35	# push ";35" for magenta forground
#	%e%p1%{3}%=%t;31	# push ";31" for red forground
#	%e%p1%{4}%=%t;36	# push ";36" for cyan forground
#	%e%p1%{5}%=%t;32	# push ";32" for green forground
#	%e%p1%{6}%=%t;34	# push ";34" for blue forground
#	%e%p1%{7}%=%t;30%;	# push ";30" for black forground
#	%?%p2%t;36;7%;		# push ";7" if reverse video
#	%?%p3%t;5%;		# push ";5" if blink
#	%?%p4%t;4%;		# push ";4" if underline
#	m			# sgr escape sequence terminator
#

#	Apple Macintosh II
#	It's taken from Macintosh II A/UX system
#
