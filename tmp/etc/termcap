#       %p1%{1}%=%t%{3} %e       # yellow
#       %p1%{2}%=%t%{5} %e       # magenta
#       %p1%{3}%=%t%{1} %e       # red
#       %p1%{4}%=%t%{6} %e       # cyan
#       %p1%{5}%=%t%{2} %e       # green
#       %p1%{6}%=%t%{4} %e       # blue
#       %p1%{7}%=%t%{0} %;       # black
######################## Fix for Bug#129315  ###################################
