	:vs=\E[*4;5h\E[0;32;40m\E[?3h:\
	:co#132:tc=id231:
ZF|id231|ID 231:\
	:is=\E[*4;5h\E[0;32;40m:\
	:vs=\E[*4;5h\E[0;32;40m:\
	:bs:pt:co#80:li#24:ms:\
	:cr=^M:do=^J:nl=^J:bl=^G:ta=^I:le=^H:\
	:al=\E[L:dl=\E[M:do=\E[B:nd=\E[C:up=\E[A:\
	:im=\E[4h:ei=\E[4l:dc=\E[P:\
	:cm=5\E[%i%d;%dH:ho=\E[H:\
	:cl=\E[;H\E[2J:ce=\E[0K:cd=\E[0J:\
	:sr=\EM:cs=\E[%i%d;%dr:\
	:so=\E[31;40m:se=\E[32;40m:us=\E[4m:ue=\E[0;32;40m:\
	:kh=\E[H:ku=\E[A:kd=\E[B:kr=\E[C:kl=\E[D:kb=\177:\
	:k0=\EOP:k1=\EOQ:k2=\EOR:k3=\EOS:\
	:l0=PF1:l1=PF2:l2=PF3:l3=PF4:\
	:HE=\EOQ:\
	:ZA=%?%p3%t%{1}%;%?%p4%t%{2}%+%;%Sz%?%Gz%Ga%=%!%t\E[0%?%p3%t;5%;%?%p4%t;4%;m%;%Gz%Sa%?%p1%{0}%=%t%{7}%e%p1%{1}%=%t%{3}%e%p1%{2}%=%t%{5}%e%p1%{3}%=%t%{1}%e%p1%{4}%=%t%{6}%e%p1%{5}%=%t%{2}%e%p1%{6}%=%t%{4}%e%p1%{7}%=%t%{0}%;%'0'%+%Sc%p1%{7}%?%p2%t%Sx%Sy%Gc%'0'%e%Sy%Sx%'0'%Gc%;%?%Gx%Gf%=%!%t\E[3%cm%e%Sd%;%?%Gy%Gb%=%!%t\E[4%cm%e%Sd%;%Gx%Sf%Gy%Sb:

# ZA=
#	%?%p3%t%{1}%;		# blink?
#	%?%p4%t%{2}%+%;		# underline?
#	%Sz
#	%?%Gz%Ga%=%!%t
#	    \E[0		# output lead in
#	    %?%p3%t;5%;		# output blink
#	    %?%p4%t;4%;		# output underline
#	    m			# end of attribute set
#	%;
#	%Gz%Sa			# save attributes
#	%?%p1%{0}%=%t%{7}	# encode color number
#	%e%p1%{1}%=%t%{3}
#	%e%p1%{2}%=%t%{5}
#	%e%p1%{3}%=%t%{1}
#	%e%p1%{4}%=%t%{6}
#	%e%p1%{5}%=%t%{2}
#	%e%p1%{6}%=%t%{4}
#	%e%p1%{7}%=%t%{0}
#	%;
#	%'0'%+%Sc
#	%p1%{7}
#	%?%p2%t%Sx%Sy%Gc%'0'	# foreground in x / background in y
#	     %e%Sy%Sx%'0'%Gc	# push background then foreground
#	%;
#	%?%Gx%Gf%=%!%t\E[3%cm%e%Sd%;	# output foreground color
#	%?%Gy%Gb%=%!%t\E[4%cm%e%Sd%;	# output background color
#	%Gx%Sf%Gy%Sb			# save colors

# ZA= \E[0;				#output lead in
# 	%?%p1%{0}%=%t%{7}		#encode color number
# 	           %e%p1%{1}%=%t%{3}
# 	           %e%p1%{2}%=%t%{5}
# 	           %e%p1%{3}%=%t%{1}
# 	           %e%p1%{4}%=%t%{6}
# 	           %e%p1%{5}%=%t%{2}
# 	           %e%p1%{6}%=%t%{4}
# 	           %e%p1%{7}%=%t%{0}%;
# 	%?%p2%t30;%{40}%+%2d		#output reverse
# 	     %e40;%{30}%+%2d%;		#output normal
# 	%?%p3%t;5%;			#output blink
# 	%?%p4%t;4%;			#output underline
#	m				#end the command
#
# end of id231
#
# The next line must be at the end of the file for "reorder" to work.
